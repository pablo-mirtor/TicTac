CREATE TABLE TRABAJADOR (ID INTEGER AUTO_INCREMENT NOT NULL, DTYPE VARCHAR(31), ACTIVO TINYINT(1) default 0, DNI VARCHAR(255), NOMBRE VARCHAR(255), SALARIO DOUBLE, VERSION INTEGER, DEPARTAMENTO_ID INTEGER, PRIMARY KEY (ID))
CREATE TABLE TRABAJADORPARCIAL (ID INTEGER NOT NULL, HORASSEMANALES INTEGER, PRIMARY KEY (ID))
CREATE TABLE TRABAJADORCOMPLETO (ID INTEGER NOT NULL, PRODUCTIVIDAD INTEGER, PRIMARY KEY (ID))
CREATE TABLE COMPETENCIA (ID INTEGER AUTO_INCREMENT NOT NULL, ACTIVO TINYINT(1) default 0, DESCRIPCION VARCHAR(255), NOMBRE VARCHAR(255), VERSION INTEGER, PRIMARY KEY (ID))
CREATE TABLE COMPETENCIATRABAJADOR (ACTIVO TINYINT(1) default 0, NIVEL INTEGER, VERSION INTEGER, COMPETENCIA_ID INTEGER NOT NULL, TRABAJADOR_ID INTEGER NOT NULL, PRIMARY KEY (COMPETENCIA_ID, TRABAJADOR_ID))
CREATE TABLE DEPARTAMENTO (ID INTEGER AUTO_INCREMENT NOT NULL, ACTIVO TINYINT(1) default 0, DESCRIPCION VARCHAR(255), NOMBRE VARCHAR(255), VERSION INTEGER, PRIMARY KEY (ID))
ALTER TABLE TRABAJADOR ADD CONSTRAINT FK_TRABAJADOR_DEPARTAMENTO_ID FOREIGN KEY (DEPARTAMENTO_ID) REFERENCES DEPARTAMENTO (ID)
ALTER TABLE TRABAJADORPARCIAL ADD CONSTRAINT FK_TRABAJADORPARCIAL_ID FOREIGN KEY (ID) REFERENCES TRABAJADOR (ID)
ALTER TABLE TRABAJADORCOMPLETO ADD CONSTRAINT FK_TRABAJADORCOMPLETO_ID FOREIGN KEY (ID) REFERENCES TRABAJADOR (ID)
ALTER TABLE COMPETENCIATRABAJADOR ADD CONSTRAINT FK_COMPETENCIATRABAJADOR_COMPETENCIA_ID FOREIGN KEY (COMPETENCIA_ID) REFERENCES COMPETENCIA (ID)
ALTER TABLE COMPETENCIATRABAJADOR ADD CONSTRAINT FK_COMPETENCIATRABAJADOR_TRABAJADOR_ID FOREIGN KEY (TRABAJADOR_ID) REFERENCES TRABAJADOR (ID)
